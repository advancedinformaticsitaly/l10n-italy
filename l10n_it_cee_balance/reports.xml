<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- PDF/HMTL REPORTS -->

    <!-- Account Report Control -->
    <report
      id="action_cee_balance_report_qweb"
      model="report_cee_balance_report"
      string="CEE Balance Report"
      report_type="qweb-pdf"
      name="l10n_it_cee_balance.cee_balance_report_qweb"
      file="l10n_it_cee_balance.cee_balance_report_qweb"
      />
    <report
      id="action_cee_balance_report_html"
      model="report_cee_balance_report"
      string="CEE Balance Report"
      report_type="qweb-html"
      name="l10n_it_cee_balance.cee_balance_report_qweb"
      file="l10n_it_cee_balance.cee_balance_report_html"
      />


    <record id="action_cee_balance_report_qweb" model="ir.actions.report">
        <field name="paperformat_id" ref="account_financial_report.report_qweb_paperformat"/>
    </record>

    <!-- XLSX REPORTS -->

    <record id="action_cee_balance_report_xlsx" model="ir.actions.report">
        <field name="name">CEE Balance Report XLSX</field>
        <field name="model">report_cee_balance_report</field>
        <field name="type">ir.actions.report</field>
        <field name="report_name">l10n_it_cee_balance.report_cee_balance_report_xlsx</field>
        <field name="report_type">xlsx</field>
        <field name="report_file">l10n_it_cee_balance.report_cee_balance_report_xlsx</field>
    </record>

</odoo>
